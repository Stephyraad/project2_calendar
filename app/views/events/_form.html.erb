<%= form_for @event do |f| %>
	Date: <%= f.text_field :date %>
	Event: <%= f.text_field :input %>
	Location: <%= f.text_field :location %>
	<%= f.select :event_type, [['Select One'],['Day Event'],['Night Event']]%>

	<%= f.submit %>
	<div class="back-btn">
		<%= button_to "Go Back ", events_path %>
  <%#= link_to 'Go Back', events_path, class: 'btn btn-default btn-lg' %>
  <div>  
<% end %>